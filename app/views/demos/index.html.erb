<h1>Listing demos</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Keywords</th>
    <th>Avg user rating</th>
    <th>Misc</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @demos.each do |demo| %>
  <tr>
    <td><%= demo.name %></td>
    <td><%= demo.description %></td>
    <td><%= demo.keywords %></td>
    <td><%= demo.avg_user_rating %></td>
    <td><%= demo.misc %></td>
    <td><%= link_to 'Show', demo %></td>
    <td><%= link_to 'Edit', edit_demo_path(demo) %></td>
    <td><%= link_to 'Destroy', demo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Demo', new_demo_path %>
